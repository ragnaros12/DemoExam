<Window x:Class="WpfApp2.Views.AddDressByUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.Views"
        mc:Ignorable="d"
        Title="AddDressByUser" Height="200" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <ComboBox Name="dress" DisplayMemberPath="material" Margin="10,20,10,20"/>
        <Button Name="Add" Click="Add_Click_1" Grid.Row="1" Margin="10,20,10,20" Content="Добавить"/>
    </Grid>
</Window>
